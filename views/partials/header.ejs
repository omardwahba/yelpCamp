    <body>
        <nav class="navbar navbar-expand-lg navbar-light bg-light fixed-top">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
              </button>
              <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <a class="navbar-brand" href="/">YelpCamp <i class="fas fa-campground"></i></a>
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item">
                        <a class="nav-link" href="/">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/campgrounds">Camps</a>
                    </li>
                </ul>
                <ul class="navbar-nav  float-md-right">
                <% if(!currentUser){ %>
                    <li class="nav-item">
                        <a class="nav-link" href="/login">Login</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/register">Signup!</a>
                    </li>
                <% } else { %>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Logged in as <strong><%= currentUser.username %></strong></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/logout">Logout</a>
                    </li>
                </ul>
                <% } %>
             </div>
        </nav>
        <div id="spacer"></div>  
        <div class="container">
        <% if(Object.keys(flash).length > 0){ %>
            <% for( key in flash){ %>
                <div class="alert alert-<%=key%>"> <%- flash[key] %></div>
            <% } %>
        <% } %>
        </div>
        
